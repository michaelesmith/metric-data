{
    "name": "michaelesmith/metric-data",
    "description": "abstract metric data storage system",
    "keywords": ["metric"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Smith",
            "email": "sitecross@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "ext-json": "*",
        "psr/log": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.4",
        "mockery/mockery": "^1.2",
        "keen-io/keen-io": "^2.6",
        "mikey179/vfsStream": "^1.6"
    },
    "suggest": {
        "keen-io/keen-io": "Allows data to be sent to the KeenIO service"
    },
    "autoload": {
        "psr-4": {"MetricData\\": "src"}
    },
    "autoload-dev": {
        "psr-4": {"MetricData\\Tests\\Unit\\": "tests/unit", "MetricData\\Tests\\Common\\": "tests/common"}
    }
